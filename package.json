{
  "name": "sixty-nine-packages",
  "version": "0.0.0",
  "description": "sixty-nine node packages",
  "author": "Daniele Barsotti",
  "private": true,
  "devDependencies": {
    "@types/node": "^11.11.3",
    "ava": "1.3.1",
    "codecov": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.0.1",
    "lerna": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "opn-cli": "^4.0.0",
    "prettier": "^1.15.2",
    "standard-version": "^5.0.1",
    "trash-cli": "^1.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.0.0",
    "typedoc": "0.14.2",
    "typescript": "^3.1.6"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@sixty-nine-packages/cache": "file:packages/cache",
    "@sixty-nine-packages/console": "file:packages/console",
    "@sixty-nine-packages/crawler": "file:packages/crawler",
    "@sixty-nine-packages/lib": "file:packages/lib",
    "@sixty-nine-packages/logger": "file:packages/logger",
    "@sixty-nine-packages/test-app": "file:packages/test-app"
  },
  "scripts": {
    "clean": "lerna run clean --stream --parallel",
    "build": "lerna run build --stream --parallel",
    "lint": "lerna run test:lint --stream --parallel",
    "pretest": "yarn lint",
    "test": "lerna link convert; lerna bootstrap; lerna run test --stream --parallel",
    "cov": "lerna run cov --stream --parallel",
    "cov:check": "lerna exec -- yarn run cov:check",
    "doc": "lerna run doc --stream --parallel"
  }
}
